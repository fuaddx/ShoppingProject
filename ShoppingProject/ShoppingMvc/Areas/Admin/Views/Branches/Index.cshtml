@model PaginationVm<IEnumerable<BranchListItemVm>>
@{
    ViewData["Title"] = "Index";
}

@section scripts {
    @if (TempData["Create"] != null)
    {
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
        @if ((bool)TempData["Create"] == true)
        {
            <script>
                Swal.fire({
                    position: "top-center",
                    icon: "success",
                    title: "Data Created",
                    showConfirmButton: false,
                    timer: 1500
                });
            </script>
        }
        else
        {
            <script>
                Swal.fire({
                    position: "top-center",
                    icon: "error",
                    title: "Something went wrong",
                    showConfirmButton: false,
                    timer: 1500
                });
            </script>
        }
    }
    @if (TempData["Delete"] != null)
    {
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
        <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>

        @if ((bool)TempData["Delete"] == true)
        {
            <script>
                // Display success message if the deletion was successful
                Swal.fire({
                    title: "Deleted!",
                    text: "Your file has been deleted.",
                    icon: "success"
                });
            </script>
        }
        else
        {
            <script>
                // Display error message if something went wrong
                Swal.fire({
                    icon: "error",
                    title: "Oops...",
                    text: "Something went wrong!",
                });
            </script>
        }
    }
    @if (TempData["Update"] != null)
    {
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
        <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>

        @if ((bool)TempData["Update"] == true)
        {
            <script>
                Swal.fire({
                    title: "Data Updated",
                    text: "Your file has been Updated",
                    icon: "success"
                });
            </script>
        }
        else
        {
            <script>
                Swal.fire({
                    icon: "error",
                    title: "Oops...",
                    text: "Something went wrong!",
                });
            </script>
        }
    }

}
<section class="content-main" id="salam">
    <div class="content-header">
        <div>
            <h2 class="content-title card-title">Branches List</h2>
        </div>
        <div>
            <a href="/Admin/Branches/Create" class="btn btn-primary btn-sm rounded">Create new</a>
        </div>
    </div>
    <div class="card mb-4">

        <div class="card-body" class="hello" id="sliderbody">
            <partial model="Model" name="BranchesPagination" />
        </div> <!-- card-body end// -->
    </div> <!-- card end// -->


</section> <!-- content-main end// -->

<footer class="main-footer font-xs">
    <div class="row pb-30 pt-15">
        <div class="col-sm-6">
            <script>
                document.write(new Date().getFullYear())
            </script> ©, Evara - HTML Ecommerce Template .
        </div>
        <div class="col-sm-6">
            <div class="text-sm-end">
                All rights reserved
            </div>
        </div>
    </div>
</footer>
