@model SellerRegistrationVm
@{
    Layout = "~/Views/Shared/_Layout2.cshtml";
}


<main class="main">
    <div class="page-header breadcrumb-wrap">
        <div class="container">
            <div class="breadcrumb">
                <a href="index.html" rel="nofollow">Home</a>
                <span></span> Pages
                <span></span> Seller Registration
            </div>
        </div>
    </div>
    <section class="pt-150 pb-150">
        <div class="container">
            <div class="row">
                <div class="col-lg-10 m-auto">
                    <div class="login_wrap widget-taber-content p-30 background-white border-radius-5">
                        <div class="padding_eight_all bg-white">
                            <div class="heading_s1">
                                <h3 class="mb-30">Be A Seller And Earn Lots Of Money</h3>
                            </div>
                            <p class="mb-50 font-sm">
                                Your personal data will be used to support your experience throughout this website, to manage access to your account, and for other purposes described in our privacy policy
                            </p>
                            <form method="post" asp-action="SellerRegistration" enctype="multipart/form-data">
                                <div class="form-group">
                                    <label asp-for="IdentityImage" class="control-label"></label>
                                    <input asp-for="IdentityImage" type="file" class="form-control-file" />
                                    <span asp-validation-for="IdentityImage" class="text-danger"></span>
                                </div>

                                <div class="form-group">
                                    <label asp-for="IdentityNumber" class="control-label"></label>
                                    <input asp-for="IdentityNumber" type="text" required="" class="form-control" placeholder="Identity Number...">
                                    <span asp-validation-for="IdentityNumber" class="text-danger"></span>
                                </div>

                                <div class="form-group">
                                    <label asp-for="TaxIdentificationNumber" class="control-label"></label>
                                    <input asp-for="TaxIdentificationNumber" type="text" required="" class="form-control" placeholder="Tax Identification Number...">
                                    <span asp-validation-for="TaxIdentificationNumber" class="text-danger"></span>
                                </div>

                                <div class="form-group">
                                    <label asp-for="ShopName" class="control-label"></label>
                                    <input asp-for="ShopName" type="text" required="" class="form-control" placeholder="Shop Name...">
                                    <span asp-validation-for="ShopName" class="text-danger"></span>
                                </div>

                                <div class="form-group">
                                    <label asp-for="ShopWebsite" class="control-label"></label>
                                    <input asp-for="ShopWebsite" type="text" required="" class="form-control" placeholder="Shop Website...">
                                    <span asp-validation-for="ShopWebsite" class="text-danger"></span>
                                </div>

                                <div class="form-group">
                                    <label asp-for="ShopDescription" class="control-label"></label>
                                    <textarea asp-for="ShopDescription" required="" class="form-control" placeholder="Shop Description..."></textarea>
                                    <span asp-validation-for="ShopDescription" class="text-danger"></span>
                                </div>

                                <div class="form-group">
                                    <label asp-for="ShopAdditionalAddressInfo" class="control-label"></label>
                                    <input asp-for="ShopAdditionalAddressInfo" type="text" class="form-control" placeholder="Additional Address Info...">
                                    <span asp-validation-for="ShopAdditionalAddressInfo" class="text-danger"></span>
                                </div>

                                <div class="form-group">
                                    <label asp-for="ShopStreet" class="control-label"></label>
                                    <input asp-for="ShopStreet" type="text" class="form-control" placeholder="Street...">
                                    <span asp-validation-for="ShopStreet" class="text-danger"></span>
                                </div>

                                <div class="form-group">
                                    <label asp-for="ShopCity" class="control-label"></label>
                                    <input asp-for="ShopCity" type="text" class="form-control" placeholder="City...">
                                    <span asp-validation-for="ShopCity" class="text-danger"></span>
                                </div>

                                <div class="form-group">
                                    <label asp-for="ShopCountry" class="control-label"></label>
                                    <input asp-for="ShopCountry" type="text" class="form-control" placeholder="Country...">
                                    <span asp-validation-for="ShopCountry" class="text-danger"></span>
                                </div>

                                <div class="form-group">
                                    <label asp-for="ShopState" class="control-label"></label>
                                    <input asp-for="ShopState" type="text" class="form-control" placeholder="State...">
                                    <span asp-validation-for="ShopState" class="text-danger"></span>
                                </div>

                                <div class="form-group">
                                    <label asp-for="ShopPostalCode" class="control-label"></label>
                                    <input asp-for="ShopPostalCode" type="text" class="form-control" placeholder="Postal Code...">
                                    <span asp-validation-for="ShopPostalCode" class="text-danger"></span>
                                </div>

                                <div class="form-group">
                                    <label asp-for="LogoImage" class="control-label"></label>
                                    <input asp-for="LogoImage" type="file" class="form-control-file" />
                                    <span asp-validation-for="LogoImage" class="text-danger"></span>
                                </div>

                                <div class="form-group">
                                    <label asp-for="ThumbnailImage" class="control-label"></label>
                                    <input asp-for="ThumbnailImage" type="file" class="form-control-file" />
                                    <span asp-validation-for="ThumbnailImage" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <button type="submit" class="btn btn-fill-out btn-block hover-up" name="login">Submit &amp; Register</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>


</main>>
